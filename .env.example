# =============================================================================
# MUIN CCTV 좌석 감지 시스템 환경 설정
# =============================================================================
# 지점별로 별도 .env 파일을 사용하거나, 하나의 .env에서 PREFIX로 구분합니다.
# 예: .env.oryudong, .env.gangnam, .env.hongdae
# 또는: 하나의 .env에서 STORE_ID로 구분
# =============================================================================

# -----------------------------------------------------------------------------
# 현재 활성 지점 설정 (필수)
# -----------------------------------------------------------------------------
# 실행할 지점 ID (데이터베이스의 store_id와 일치해야 함)
STORE_ID=oryudong

# -----------------------------------------------------------------------------
# 지점별 RTSP 설정
# -----------------------------------------------------------------------------
# 형식: {STORE_ID}_RTSP_* (대문자)
# 지점별로 다른 RTSP 서버 사용 시 설정

# 오류동역점
ORYUDONG_RTSP_HOST=192.168.1.100
ORYUDONG_RTSP_PORT=8554
ORYUDONG_RTSP_USERNAME=admin
ORYUDONG_RTSP_PASSWORD=password123
ORYUDONG_ACTIVE_CHANNELS=1,2,3,4

# 강남점 (예시)
# GANGNAM_RTSP_HOST=192.168.2.100
# GANGNAM_RTSP_PORT=8554
# GANGNAM_RTSP_USERNAME=admin
# GANGNAM_RTSP_PASSWORD=password456
# GANGNAM_ACTIVE_CHANNELS=1,2,3,4,5,6

# 홍대점 (예시)
# HONGDAE_RTSP_HOST=192.168.3.100
# HONGDAE_RTSP_PORT=8554
# HONGDAE_RTSP_USERNAME=admin
# HONGDAE_RTSP_PASSWORD=password789
# HONGDAE_ACTIVE_CHANNELS=1,2,3,4

# -----------------------------------------------------------------------------
# 기본 RTSP 설정 (지점별 설정이 없을 때 사용)
# -----------------------------------------------------------------------------
RTSP_USERNAME=admin
RTSP_PASSWORD=your_password
RTSP_HOST=your_rtsp_host
RTSP_PORT=8554

# 활성 채널 (기본값, 지점별로 덮어쓸 수 있음)
ACTIVE_CHANNELS=1,2,3,4

# -----------------------------------------------------------------------------
# 모델 설정
# -----------------------------------------------------------------------------
YOLO_MODEL=yolo11n.pt
CONFIDENCE_THRESHOLD=0.3
IOU_THRESHOLD=0.3

# -----------------------------------------------------------------------------
# 처리 설정
# -----------------------------------------------------------------------------
SNAPSHOT_INTERVAL=3
MAX_WORKERS=4

# -----------------------------------------------------------------------------
# API 서버 설정
# -----------------------------------------------------------------------------
API_HOST=0.0.0.0
API_PORT=8001

# -----------------------------------------------------------------------------
# 외부 서비스
# -----------------------------------------------------------------------------
# GoSca 좌석 관리 시스템
GOSCA_BASE_URL=https://gosca.co.kr
GOSCA_STORE_ID=Anding-Oryudongyeok-sca

# Supabase (https://supabase.com)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_KEY=your_anon_public_key
SUPABASE_SERVICE_KEY=your_service_role_key

# -----------------------------------------------------------------------------
# 로컬 테스트용 설정
# -----------------------------------------------------------------------------
# DEBUG=true
# LOG_LEVEL=DEBUG
# DRY_RUN=true  # DB에 저장하지 않고 로그만 출력
